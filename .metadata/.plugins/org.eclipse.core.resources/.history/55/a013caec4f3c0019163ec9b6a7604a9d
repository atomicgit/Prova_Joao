package Prova.prova;

import java.util.ArrayList;

public class Locadora {
	
	private ArrayList<String> Diretores = new ArrayList();
	private ArrayList<String> filmes = new ArrayList();
	
	public Diretores getDiretores(int posicao){
	    return this.diretores[posicao];
	}
	public Diretores getDiretores(){
	    return this.diretores = diretores;
	}
	
}
